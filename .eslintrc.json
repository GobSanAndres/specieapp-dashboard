{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": "off",
        "import/prefer-default-export": "off",
        "no-useless-constructor":"off",
        "no-unused-vars":"warn",
        "no-empty-function":"warn",
        "import/no-unresolved": "off",
        "comma-dangle":"warn",
        "class-methods-use-this":"off",        
        "import/extensions":"off"
    }
}
